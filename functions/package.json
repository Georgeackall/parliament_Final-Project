{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "postinstall": "node node_modules/puppeteer/install.js",
    "gcp-build": "node node_modules/puppeteer/install.js",
    "serve": "lsof -ti :8085 | xargs -r kill -9; lsof -ti :8080 | xargs -r kill -9; firebase emulators:start --inspect-functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "predeploy": "npm run gcp-build",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "20"
  },
  "main": "index.js",
  "dependencies": {
    "@extractus/article-extractor": "^7.3.1",
    "@google-cloud/pubsub": "^4.3.3",
    "@pinecone-database/pinecone": "^2.1.0",
    "@google-cloud/firestore": "^7.6.0",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^4.9.0",
    "lodash": "^4.17.21",
    "natural": "^6.12.0",
    "openai": "^4.28.4",
    "puppeteer": "^19.11.1",
    "url-metadata": "^3.5.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "^8.15.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.2.0"
  },
  "private": true
}